<description>
    <executable name="agent.sh">
        <access
            type="LFN">
            <value value="/grid/vlemed/eduard/bin/agent.sh"/>
        </access>
	
        <sandbox name="jade">
            <access type="LFN"/>
            <value value="/grid/vlemed/eduard/bin/jade.jar"/>
        </sandbox>
	
        <sandbox name="amc">
            <access type="LFN"/>
            <value value="/grid/vlemed/eduard/bin/DUST-AMC.jar"/>
        </sandbox>
        <output name="stdout" option=">">
            <template value="/grid/vlemed/eduard/tmp/agent-out-%s.txt"/>
            <access type="LFN"/>
        </output>
        <output name="stderr" option="2>">
            <template value="/grid/vlemed/eduard/tmp/agent-err-%s.txt"/>
            <access type="LFN"/>
        </output>

    </executable>
</description>